    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

    <style>
        body {
            padding-top: 80px;
            word-wrap: break-word;
            /*            font-size: 90%;*/
            font-family: Arial, Helvetica, sans-serif;
        }
        
        .exam_details h2 {
            color: #1B7FB9;
            font-size: 3rem;
        }
        
        .exam_details h2 i,
        .exam_details h2 span {
            color: #7EAB51;
        }
        
        .border_tb {
            border-top: 2px solid #F5F5F5;
            border-bottom: 2px solid #F5F5F5;
            padding-top: 10px;
            padding-bottom: 30px;
        }
        
        .left_side,
        .right_side {
            border: 2px solid #E8E8E8;
            background-color: #F5F5F5;
            padding: 15px;
        }
        
        .left_side {
            margin-right: 10px;
        }
        
        .left_side h2 {
            border: 1px solid #96c765;
            margin: 0;
            line-height: 3rem;
            padding: 10px 0;
            border-radius: 20px;
            /*margin: 15px;*/
            /*Gradiant color style*/
            background: #96c765;
            background: -moz-linear-gradient(top, #96c765 0%, #577d31 100%);
            background: -webkit-gradient(left top, left bottom, color-stop(0%, #96c765), color-stop(100%, #577d31));
            background: -webkit-linear-gradient(top, #96c765 0%, #577d31 100%);
            background: -o-linear-gradient(top, #96c765 0%, #577d31 100%);
            background: -ms-linear-gradient(top, #96c765 0%, #577d31 100%);
            background: linear-gradient(to bottom, #96c765 0%, #577d31 100%);
            filter: progid: DXImageTransform.Microsoft.gradient( startColorstr='#96c765', endColorstr='#577d31', GradientType=0);
        }
        
        .right_side h2 {
            border: 1px solid #F79722;
            margin: 0;
            line-height: 3rem;
            padding: 10px 0;
            border-radius: 20px;
            /*margin: 15px;*/
            /*Gradiant color style*/
            background: #fcb660;
            background: -moz-linear-gradient(top, #fcb660 0%, #f7941b 100%);
            background: -webkit-gradient(left top, left bottom, color-stop(0%, #fcb660), color-stop(100%, #f7941b));
            background: -webkit-linear-gradient(top, #fcb660 0%, #f7941b 100%);
            background: -o-linear-gradient(top, #fcb660 0%, #f7941b 100%);
            background: -ms-linear-gradient(top, #fcb660 0%, #f7941b 100%);
            background: linear-gradient(to bottom, #fcb660 0%, #f7941b 100%);
            filter: progid: DXImageTransform.Microsoft.gradient( startColorstr='#fcb660', endColorstr='#f7941b', GradientType=0);
        }
        
        .right_side {
            margin-left: 10px;
        }
        
        .left_side h2 {
            text-align: center;
            color: #fff;
            font-size: 2rem;
        }
        
        .left_side h2 i {
            font-size: 4rem;
            color: #fff;
        }
        
        .right_side h2 {
            text-align: center;
            color: #fff;
            font-size: 2rem;
        }
        
        .right_side h2 i {
            font-size: 4rem;
            color: #fff;
        }
        
        .left_side ul,
        .right_side ul {
            padding: 0;
            margin-top: 20px;
        }
        
        .left_side ul li,
        .right_side ul li {
            color: #333;
            list-style: none;
            font-size: 1.5rem;
        }
        
        .left_side ul li i {
            color: #7EAB51;
        }
        
        .right_side ul li i {
            color: #FFAB38;
        }

    </style>
</head>

